
# Setup Advent of Code aliases
if [[ -f ~/src/advent/python/aliases.sh ]]; then
   source ~/src/advent/python/aliases.sh
fi

# bat - colorized cat, based on file type
alias bat="batcat --style snip --theme 'Visual Studio Dark+'"
alias cat=bat

alias qflipper=~/src/flipperzero/qFlipper/build/qFlipper-x86_64.AppImage

alias gitdiff=ydiff

